#include <iostream>
#include <bits/stdc++.h>
using namespace std;
long long int max(long long int a, long long int b)
  {
		if(a > b)
			return a;
		else
			return b;
	}
int main()
{
     long long int n,i,x,t,j,jj,mi;
long long int v[10009],w[10009];

    scanf("%lld",&t);
    for(jj=1;jj<=t;jj++)
    {
        scanf("%lld",&n);
        if(n>2)
   {

    for(i=0;i<n;i++)
    {
        scanf("%lld",&v[i]);

    }
    w[0]=v[0];
    w[1]=max(v[0],v[1]);
    for(i=2;i<n;i++)
    {
         mi=-1;
        for(j=0;j<i-1;j++)
       { mi=max(w[j]+v[i],mi);}
        w[i]=max(mi,w[i-1]);
    }


printf("Case %lld: %lld\n",jj,w[n-1]);
   }
   else if(n==2)
   {
       for(i=0;i<n;i++)
    {
        scanf("%lld",&v[i]);

    }
       printf("Case %lld: %lld\n",jj,max(v[0],v[1]));

   }
   else if(n==1)
   {
        scanf("%lld",&x);


        printf("Case %lld: %lld\n",jj,x);
   }
   else

{ char c[64];
cin.getline(c,64);
 printf("Case %lld: 0\n",jj);}
 }
    return 0;
}
