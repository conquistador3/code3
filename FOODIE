#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int c,l,W,n,x,y,pp,i,w,lo,hi,mid,t,tt,xx,s=0,a;
    vector <int> val,wt;

    scanf("%d",&t);
    for(tt=0;tt<t;tt++)
    {
        scanf("%d%d",&n,&W);
    for(pp=0;pp<n;pp++)
        {
            scanf("%d",&x);
            s=0;
            for(xx=0;xx<x;xx++)
            {
                            scanf("%d",&a);
                            s=s+a;
            }
            cout<<s<<endl;
            wt.push_back(s);
            val.push_back(s);

        }
        vector <vector<int> > K(n+2, vector<int> (W+5, 0));

        for (i = 0; i <= n; i++)
   {
       for (w = 0; w <= W; w++)
       {
           if (i==0 || w==0)
               K[i][w] = 0;
           else if (wt[i-1] <= w)
                 K[i][w] = max(val[i-1] + K[i-1][w-wt[i-1]],K[i-1][w]);
           else
                 K[i][w] = K[i-1][w];
       }
   }
printf("%d \n",K[n][W]);

   wt.clear();
   val.clear();
    }
    return 0;
}
