#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main()
{
    vector <int> v;
    int i,j,m,k,y,yy,x;
v.push_back(0);
v.push_back(1);
v.push_back(2);
v.push_back(3);
v.push_back(1);
v.push_back(2);
v.push_back(3);
v.push_back(4);
v.push_back(2);
v.push_back(1);
v.push_back(2);
v.push_back(3);
v.push_back(3);
v.push_back(2);
v.push_back(3);
for(i=15;i<=1004;i++)
{
    if(sqrt(i)==(int)sqrt(i))
    {
        v.push_back(1);
    }
    else
    {
        m=10000000;
        j=1;

       for (j=1; j<=i/2; j++)
   {
    m=min(v[j]+v[i-j] ,m);

   }

        v.push_back(m);
    }
}
scanf("%d",&yy);
for(y=0;y<yy;y++)
{
    scanf("%d",&x);
    printf("%d\n",v[x]);
}
    return 0;
}
